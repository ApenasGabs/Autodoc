{
  "name": "autodoc",
  "version": "0.1.0",
  "description": "Gerador de documentação automática para repositórios GitHub usando IA",
  "main": "dist/index.js",
  "scripts": {
    "start": "node --max-old-space-size=8192 dist/index.js",
    "dev": "NODE_OPTIONS=--max-old-space-size=8192 ts-node src/index.ts",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint src --ext .ts",
    "test": "jest"
  },
  "author": "Seu Nome <seu.email@exemplo.com>",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^19.0.7",
    "axios": "^1.4.0",
    "dotenv": "^16.0.3",
    "glob": "^10.2.6",
    "marked": "^5.0.2",
    "simple-git": "^3.19.0"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.1",
    "@types/marked": "^5.0.0",
    "@types/node": "^20.1.4",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
